{
  "name": "codinit-backend",
  "version": "1.0.0",
  "description": "Backend API for CodInit platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "build:serverless": "tsc && cp -r dist/* dist/serverless/",
    "start:serverless": "node dist/serverless.js",
    "lint": "eslint src --ext .ts",
    "vercel": "vercel dev",
    "deploy:vercel": "vercel --prod"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "latest",
    "@ai-sdk/fireworks": "latest",
    "@ai-sdk/google": "latest",
    "@ai-sdk/google-vertex": "latest",
    "@ai-sdk/mistral": "latest",
    "@ai-sdk/openai": "latest",
    "@e2b/code-interpreter": "^1.0.2",
    "@flags-sdk/growthbook": "^0.1.0",
    "@google-cloud/aiplatform": "^4.2.0",
    "@google-cloud/storage": "^7.16.0",
    "@huggingface/hub": "^2.3.0",
    "@supabase/supabase-js": "^2.50.3",
    "@upstash/ratelimit": "^2.0.1",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/kv": "^2.0.0",
    "@xenova/transformers": "^2.17.2",
    "ai": "latest",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jszip": "^3.10.1",
    "node-fetch": "^3.3.2",
    "ollama-ai-provider": "latest",
    "serverless-http": "^3.2.0",
    "stripe": "^18.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^22.16.3",
    "@types/node-fetch": "^2.6.12",
    "@vercel/node": "^4.0.0",
    "eslint": "^8",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4",
    "vercel": "^28.18.5"
  }
}
